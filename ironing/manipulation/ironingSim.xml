<application>
<name>ironingSim App</name>

    <module>
        <name>teoSim</name>
        <parameters>--env /home/yo/repos/textiles/ironing/manipulation/ironingSim/ironingSim.env.xml</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>launchTeoYarp</name>
        <parameters>--from /usr/local/share/teo/contexts/kinematics/rightArmKinematics-pan45-tilt30.ini --device BasicCartesianControl --angleRepr axisAngle --robot remote_controlboard --local /BasicCartesianControl/teoSim/rightArm --remote /teoSim/rightArm</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>mover</name>
        <parameters>--robot /teoSim --forceThreshold 0.000001</parameters>
        <node>localhost</node>
    </module>

</application>

