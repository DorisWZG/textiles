<application>
<name>ironing App</name>

    <module>
        <name>launchTeoYarp</name>
        <parameters>--from /usr/local/share/teo/contexts/kinematics/rightArmKinematics.ini --device BasicCartesianControl --angleRepr axisAngle --robot remote_controlboard --local /BasicCartesianControl/teo/rightArm --remote /teo/rightArm</parameters>
        <node>localhost</node>
    </module>

    <connection>
        <from>/jr3ch3:o</from>
        <to>/CartesianControl/force:i</to>
    </connection>

    <module>
        <name>mover</name>
        <parameters>--robot /teo</parameters>
        <node>localhost</node>
    </module>

</application>

